 /* Imagem da capa - v300 * /
 if v300>'' then
		'<7000 0>Capa:</7000>'
		'<7001 0>&#160;</7001>'
		'<7002 0>',replace(v300^a[last],'veja ',''),' (arquivo: ',v300^b[last],') </7002>'
 fi */
 
