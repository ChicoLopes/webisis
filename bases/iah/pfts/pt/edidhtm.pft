                '<7000 0><b>Localiza&ccedil;&atilde;o</b>:</7000>'
                '<7001 0>&#160;</7001>'
                '<7002 0>'
                        if v202>'' then
 				v202
 				replace(s(" / Arm&aacute;rio: "v21),'/','-')
 				replace(s(" / Prateleira: "v22),'/','-')
